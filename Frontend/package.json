{
  "name": "motive",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:ssl": "ng serve --ssl true",
    "restart": "npm restart",
    "test-pwa": "http-server -c-1 --proxy https://localhost:3000 .\\dist\\motive",
    "prod-build": "ng build --prod --aot --output-path \".\\dist\\prod\" --base-href=/",
    "onPrem-build": "ng build --prod --configuration=onPrem --base-href=/secutil/",
    "qa-build": "ng build --configuration=qa --output-path \".\\dist\\qa\" --base-href=/virtual-path/",
    "dev-build": "ng build --prod --configuration=dev --base-href=/submission/",
    "uat-build": "ng build --configuration=uat --output-path \".\\dist\\uat\" --base-href=/virtual-path/",
    "build": "npm run prod-build && npm run qa-build && npm run uat-build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.1.2",
    "@angular/cdk": "^11.1.2",
    "@angular/common": "~11.1.2",
    "@angular/compiler": "~11.1.2",
    "@angular/core": "~11.1.2",
    "@angular/forms": "~11.1.2",
    "@angular/platform-browser": "~11.1.2",
    "@angular/platform-browser-dynamic": "~11.1.2",
    "@angular/router": "~11.1.2",
    "@angular/service-worker": "~11.1.2",
    "@ionic/angular": "^5.6.13",
    "@ngrx/effects": "^10.1.2",
    "@ngrx/entity": "^10.1.2",
    "@ngrx/store": "^10.1.2",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@okta/okta-angular": "^3.0.1",
    "ag-grid-angular": "^25.0.1",
    "ag-grid-community": "^25.0.1",
    "ag-grid-enterprise": "^25.0.1",
    "moment": "^2.29.1",
    "ngx-dropzone": "^3.0.0",
    "ngx-perfect-scrollbar": "^10.1.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1101.4",
    "@angular/cli": "~11.1.4",
    "@angular/compiler-cli": "~11.1.2",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/service-worker": "^11.1.2",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "autoprefixer": "^10.3.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.2",
    "@ionic/angular-toolkit": "latest"
  }
}
